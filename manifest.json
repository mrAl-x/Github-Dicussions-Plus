{
  "name": "Github Better Review",
  "version": "0.1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://github.com/**/**/pull/**/files"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_title": "navigation"
  },
  "web_accessible_resources": ["assets/*.png"],
  "permissions": ["activeTab"]
}
