{
  "name": "github",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_title": "navigation"
  },
  "web_accessible_resources": ["assets/*.png"],
  "permissions": ["activeTab"]
}
